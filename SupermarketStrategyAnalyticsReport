   "source": [
    "##### Customer reordered status"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 137,
   "id": "4efb829b",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      " Reordered Status  Proportion (%)  Total Users (Desc Order)\n",
      "             True              59                   1190986\n",
      "            False              41                    828515\n"
     ]
    }
   ],
   "source": [
    "# Summary Table: Cusomter reordered status\n",
    "\n",
    "summary_table_reordered = df['reordered'].value_counts(normalize=True).reset_index()\n",
    "\n",
    "# Rename columns\n",
    "summary_table_reordered.columns = ['Reordered Status', 'Proportion (%)']\n",
    "\n",
    "# Convert proportion to percentage and round to whole number\n",
    "summary_table_reordered['Proportion (%)'] = (summary_table_reordered['Proportion (%)'] * 100).round().astype(int)\n",
    "\n",
    "# Value counts in a separate column\n",
    "summary_table_reordered['Total Users (Desc Order)'] = df['reordered'].value_counts().values\n",
    "\n",
    "# Print the DataFrame without showing the index\n",
    "print(summary_table_reordered.to_string(index=False))"
   ]
